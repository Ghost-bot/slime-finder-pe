{
  "name": "slime-finder-pe",
  "version": "1.0.0",
  "description": "Web app to locate slime chunks in MCPE",
  "repository": {
    "type": "git",
    "url": "https://github.com/depressed-pho/slime-finder-pe.git"
  },
  "scripts": {
    "build": "run-p webpack copy:html",
    "webpack": "webpack",
    "copy:html": "cpx html/**/*.html dist",
    "watch:html": "cpx --watch html/**/*.html dist",
    "clean": "rimraf dist",
    "test": "mocha --compilers ts:ts-node/register -r tsconfig-paths/register -r ts-node/register test/**/*.ts",
    "deploy": "run-s build deploy:gh-pages",
    "deploy:gh-pages": "gh-pages -d dist",
    "start": "run-p watch:html server",
    "server": "webpack-dev-server --content-base dist"
  },
  "author": "PHO <pho@cielonegro.org>",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/baconjs": "^0.7.31",
    "@types/chai": "^3.5.0",
    "@types/hammerjs": "^2.0.34",
    "@types/mocha": "^2.2.40",
    "@types/underscore": "^1.8.0",
    "bootstrap-loader": "^2.0.0",
    "chai": "^3.5.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "gh-pages": "^0.12.0",
    "imports-loader": "^0.7.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "ts-loader": "^2.0.3",
    "ts-node": "^3.0.2",
    "tsconfig-paths": "^2.1.2",
    "typescript": "^2.2.2",
    "uglify-js": "^2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "baconjs": "^0.7.92",
    "bootstrap-sass": "^3.3.7",
    "hammerjs": "^2.0.8",
    "mersenne-twister": "^1.1.0",
    "underscore": "^1.8.3"
  }
}
